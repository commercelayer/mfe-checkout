{
  "name": "commercelayer-react-checkout",
  "version": "1.7.5",
  "engines": {
    "node": ">=14.x",
    "yarn": "1.x"
  },
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "lint": "eslint ./ --ext .js,.jsx,.tsx,.ts",
    "lint:fix": "eslint ./ --ext .tsx,.ts --fix",
    "test": "playwright test",
    "test:debug": "PWDEBUG=1 playwright test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commercelayer/commercelayer-react-checkout.git"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      [
        "@semantic-release/github",
        {
          "addReleases": "top"
        }
      ]
    ]
  },
  "author": {
    "name": "Matteo Alessani",
    "email": "matteo@commercelayer.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/commercelayer/commercelayer-react-checkout/issues"
  },
  "homepage": "https://github.com/commercelayer/commercelayer-react-checkout#readme",
  "keywords": [
    "reactjs",
    "nextjs",
    "jamstack",
    "headless",
    "ecommerce",
    "api",
    "checkout",
    "commercelayer"
  ],
  "dependencies": {
    "@commercelayer/react-components": "^3.12.1",
    "@headlessui/react": "^1.6.1",
    "@rollbar/react": "^0.11.0",
    "async-retry": "^1.3.3",
    "classnames": "^2.3.1",
    "jwt-decode": "^3.1.2",
    "next": "^12.1.6",
    "next-i18next": "^11.0.0",
    "pure-md5": "^0.1.13",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-gtm-module": "^2.0.11",
    "rollbar": "^2.25.0",
    "styled-components": "^5.3.5",
    "swr": "^1.3.0",
    "twin.macro": "2.8.2"
  },
  "devDependencies": {
    "@commercelayer/js-auth": "^2.3.0",
    "@faker-js/faker": "^6.3.1",
    "@next/bundle-analyzer": "^12.1.6",
    "@next/eslint-plugin-next": "^12.1.6",
    "@playwright/test": "^1.21.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.4",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@tailwindcss/forms": "^0.5.1",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@types/async-retry": "1.4.3",
    "@types/node": "^17.0.31",
    "@types/react": "^17.0.45",
    "@types/react-gtm-module": "^2.0.1",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "autoprefixer": "^10.4.7",
    "dotenv": "^16.0.0",
    "eslint": "^8.15.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-tailwind": "^0.2.1",
    "postcss": "^8.4.13",
    "prettier": "^2.6.1",
    "stylelint": "^14.8.2",
    "stylelint-config-standard": "^25.0.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.4"
  },
  "resolutions": {
    "@types/react": "17.0.45"
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components"
    }
  },
  "cacheDirectories": [
    ".next/cache"
  ]
}
